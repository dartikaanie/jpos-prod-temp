<?xml version="1.0" encoding="UTF-8"?>
<channel-adaptor name='bri-host-with-header' class="org.jpos.q2.iso.ChannelAdaptor"
	logger="Q2">
	  <channel class="org.jpos.iso.channel.NACChannel"  logger="Q2" packager="org.jpos.iso.packager.GenericPackager">
        <property name="packager-config" value="cfg/packager/iso87binary.xml" />
        <property name="host" value="172.16.194.26" />  #-- server bri 172.16.194.26 -- -- local 172.16.194.26
        <property name="port" value="7011" />  #local 7011 -- server 6011
        <property name="keep-alive" value="true" />
        <property name="timeout" value="2700000"/>
	  </channel>
	  <max-connection>8</max-connection>
	  <in>bri-host-with-header-send</in>
	  <out>bri-host-with-header-receive</out>
    	<unhandled>bri-host-with-header-unhandled</unhandled>
    <ready>bri-host-with-header.ready</ready>
	  <reconnect-delay>8000</reconnect-delay>
</channel-adaptor>