<mux class="org.jpos.q2.iso.QMUX" logger="Q2" name="bri-host-with-header-mux">
    <in>bri-host-with-header-receive</in>
    <out>bri-host-with-header-send</out>
    <ready>bri-host-with-header.ready</ready>
    <unhandled>bri-host-with-header-unhandled</unhandled>
    <request-listener class="org.pcs.jpos.iso.SocketISORequestHandler" logger="Q2" realm="connector">
        <property name="txnmgr" value="txnmgr" />
